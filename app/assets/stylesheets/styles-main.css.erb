@font-face {
  font-family: 'oswaldlight';
  src: url('<%= asset_path 'oswald-light-webfont.eot' %>');
  src: url('<%= asset_path 'oswald-light-webfont.eot?#iefix' %>') format('embedded-opentype'),
       url('<%= asset_path 'oswald-light-webfont.woff' %>') format('woff'),
       url('<%= asset_path 'oswald-light-webfont.ttf' %>') format('truetype'),
       url('<%= asset_path 'oswald-light-webfont.svg#oswaldlight' %>') format('svg');
  font-weight: normal;
  font-style: normal;
}

@font-face {
  font-family: 'oswaldregular';
  src: url('<%= asset_path 'oswald-regular-webfont.eot' %>');
  src: url('<%= asset_path 'oswald-regular-webfont.eot?#iefix' %>') format('embedded-opentype'),
       url('<%= asset_path 'oswald-regular-webfont.woff' %>') format('woff'),
       url('<%= asset_path 'oswald-regular-webfont.ttf' %>') format('truetype'),
       url('<%= asset_path 'oswald-regular-webfont.svg#oswaldlight' %>') format('svg');
  font-weight: normal;
  font-style: normal;
}

html{
  width:100%;
  height:100%;
}

body {
  margin: 0px;
  padding: 0px;
  font-family: "Verdana, Helvetica, Arial, sans-serif";
  height:100%;
}

a {
  font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
}

.header {
  margin-left: auto ;
  margin-right: auto ;
  width: 100%;
  height: 110px;
  background-color: #e3e3e6;
  border-bottom: 4px solid #dbd7db;
}

.header .lang {
  float: right;
  margin-top: 4px;
  margin-right: 20px;
  color: #E9F1F4;
}

.header .lang a {
  color: #E9F1F4;
  font-size: 11px;
}

.header .header-top1 {
  width: 100%;
  min-width: 960px;
  height: 40px;
  background-color: #2D2D2D;
}

.header .header-top2 {
  width: 100%;
  height: 69px;
  border-bottom: 1px solid #dddddd;
  border-top: 1px solid #dddddd;
}

.photo {
  width: 50px;
  height: 50px;
  float: left;
  margin-right: 5px;
  margin-left: 5px;
  margin-top: 5px;
}

.details {
  width: 140px;
  text-align: left;
  float: left;
}

.details ul {
  color: black;
  font-style: italic;
  list-style-type: none;
  font-size: 10.5px;
  margin-left: 0px;
}

.details ul li{
  line-height: 14px;
}

.details h1 {
  font-size: 20px;
  
  font-family: "oswaldlight";
  margin-top: 7px;
  line-height: 25px;
  -webkit-text-shadow: 2px 2px 2px #aaa;
  -moz-text-shadow: 2px 2px 2px #aaa;
  text-shadow: 2px 2px 2px #aaa;
}

.icon {
  width: 20px;
  height: 20px;
  float:left;
  margin-top: 47px;
  margin-left: 2px;
}

.content {
  text-align: left;
  width: 84%;
  max-width: 100%;
  min-width: 652px;
  float: left;
  min-height: 300px;
  padding-top: 5px;
  padding-bottom: 20px;
}

.section-content {
  margin-left: 20px;
  margin-right: 15px;
}


.pages-path {
  min-width: 783px;
  width: 100%;
  height: 30px;
  background-color: #F1F1F7;
  border: 1px solid #dddddd;
  border-bottom: 1px solid #dddddd; 
  margin-top: 10px;
}

.pages-path .pages-path-item {
  float:left;
  padding-right:4px;
  padding-bottom: 4px;
  font-size: 12px;
  padding-top: 6px;
  padding-left: 0px;
          
}

.pages-path .separator {
  margin: auto; 
  width: 100%; 
  height: 1px; 
  border-top: 1px solid #dddddd; 
  clear: both;
}

#loading {
  width: 30px;
  height: 30px;
  float: right;
  
  display: none;
  position:absolute;
  z-index: 2;
}

#loading img {
  margin: auto;
  margin-left: 92px;
  margin-top: 25px;
}

.tryluck {
  width: 98%;
  min-width: 800px;
  margin-top: 20px;
  overflow: auto;
  margin-left: 5px;
}

.block {
  width: 100%;
  min-width: 800px;
  overflow: auto;
}

.block .item-image {
  height: 100px;
  width: 100px;
  vertical-align: middle;
  margin: 0 auto;

  text-align: center;

  -webkit-transition: all .5s ease-in-out;
  -moz-transition: all .5s ease-in-out;
  transition: all .5s ease-in-out;
}

.block .item-image img {

}

.block .item-description {
  float: left;
  width: 70%;
}

.block .item-code {
  display: none;
}





.footer {
  margin-left: auto ;
  margin-right: auto ;
  margin-bottom: 15px;
  text-align: center;
  width: 100%;
  height: 120px;
  border-top: 1px solid #dddddd;
  border-bottom: 1px solid #dddddd;
  background-color: #F1F1F7;
  clear: both;
  padding-top: 10px;
}

.footer-content {
  text-align: center;
  margin: auto;
  width: 610px;
  height: 105px;
}

.footer-item {
  text-align: left;
  height: 100px;
  width: 140px;
  float: right;
  margin-left: 10px;
}

.footer-item span {
  font-weight: bold;
  font-size: 12px;
  width: 100px;
  
}

.footer-item ul {
  margin: 0px;
  padding-top: 7px;
  border-top: 1px solid #dddddd;
}

.footer-item ul li {
  font-size: 11px;
  list-style-type: none;
  line-height: 14px;
}

.footer-item ul li a:hover {
  text-decoration: none;
}

.maincontent {
  width: 100%;
  min-width: 774px;
  overflow: auto;
}

pre {
  width: 100%;
  height: 100%;
}

.rating {
  width: 200px;
  height: 100px;
  margin: auto;
  position: fixed;
  bottom: 10px;
  right: 10px;
  display:none;
  background-color: ghostwhite;
  
  text-align: center;

  /*-webkit-border-radius: 5px;
  -moz-border-radius: 5px;
  border-radius: 5px;

  -moz-box-shadow: 0px 0px 5px #2D2D2D;
  -webkit-box-shadow: 0px 0px 5px #2D2D2D;
  box-shadow: 0px 0px 5px #2D2D2D;*/

  font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
  font-size: 13px;
  font-weight: bold;
}

.rating-header {
  width: 100%;
  height: 20px;
  background-color: #2D2D2D;
  /*-moz-border-radius-topleft: 3px;
  -moz-border-radius-topright: 3px;

  -webkit-border-top-left-radius: 3px;
  -webkit-border-top-right-radius: 3px;

  border-top-left-radius: 3px;
  border-top-right-radius: 3px;*/
}

.rating-header button{
  margin-right: 5px;
  margin-top: 1px;
}

.rating-content {
  border: 1px solid #dddddd;
  overflow: auto;
}

.rating-container {
  width: 180px;
  height: 60px;
  background: white;
  margin: auto;
  background-color: ghostwhite;
  
}

.rating-name {
  width: 100%;
  height: 20px;
  margin-top: 17px;
  text-align: center;
  color: #2D2D2D;
}

.rating-stars {
  height: 30px;
  width: 120px;
  margin: auto;
  margin-left: 32px;
}

.rating-thanks {
  height: 50px;
  width: 120px;
  margin-left: 28px;
  margin-top: 3px;
  padding-top: 20px;
  display: none;
}

#chat {
  width: 300px;
  height: 0px;
  float:right;
  position: absolute;
  right: 2px;
  top: 70px;
  text-align: left;
  margin-right: 0px;
  z-index: 10;
  display: none;
  /*-moz-box-shadow: 0px 0px 5px #2D2D2D;
  -webkit-box-shadow: 0px 0px 5px #2D2D2D;
  box-shadow: 0px 0px 5px #2D2D2D;*/
}

#chat-name {
  width: 300px;
  height: 20px;
  background-color: #2D2D2D;
  border: 1px solid #2D2D2D;
  padding-top: 4px;
  cursor: pointer;
}

#chat-container {
  display: none;
}

.chat-messages {
  overflow: auto;
  width: 100%;
  height: 200px;
  background-color: ghostwhite;
  border: 1px solid #dddddd;
}

.chat-message {
  width: 100%;
  height: 100px;
}

.chat-message textarea{
  width: 292px;
  height: 50px;
}

#chat-is-typing {
  height: 15px;
  background-color: #dddddd;
  color: #666666;
  font-size: 10px;
  padding-left: 5px;
  font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
  display: none;
}

.msg {
  padding: 5px;
  font-size: 10px;
  border-bottom: 1px solid #dddddd;
  min-height: 35px;
}

.chat-button {
  color: white;
  margin-right: 5px;
}

#chat-title {
  width: 100px;
  float: left;
  margin-left: 10px;
  color: white;
  font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
  font-size: 11px;
}

#chat-nickname {
  float: left;
  width: 100px;
  height: 10px;
  color: white;
  font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
  font-size: 11px;
}

#chat-nickname input {
  width: 100px;
  height: 6px;
  line-height: 7px;
  font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
  font-size: 11px;
}

#chat-visibility {
  float:right;
  width: 40px;
  font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
  font-size: 11px;
}

#chat-visibility a{
  color: white;
}

.corner {
    background-color: #a00;
    overflow: hidden;
    position: relative;
    float: right;
    right: -19px;
    bottom: 17px;
    text-align: center;
    height: 15px;
    width: 55px;
    color: white;
    -moz-transform: rotate(-45deg);
    -webkit-transform: rotate(-45deg);
    transform: rotate(-45deg);

    -moz-box-shadow: 0 0 1em #888;
    -webkit-box-shadow: 0 0 1em #888;
    box-shadow: 0 0 1em #888;
}
.corner a {
    border: 1px solid #faa;
    color: #fff;
    display: block;
    font-size:.8em;
    font-weight:bold;
    text-align: center;
    text-decoration: none;
    text-shadow: 1px 1px 1px #000;
    padding: 0px;
}

.header-right-buttons {
  position: absolute;
  float: right;
  margin-top: 3px;
  right: 0px;
}

.right-buttons {
  float: right;
  position:relative;
  margin-right: 0px;
  margin-top: 3px;
  text-align: right;
}

.facebook-button {
  float: right;
  margin-right: 10px;
}

.twitter-button {
  margin-right: 10px;
  float: right;
  text-align: right;
}

.googleplus-button {
  width: 70px;
  height: 15px;
  text-align: right;
}

.aboutme-tools {
  margin: 5px;
  float: left;
}
